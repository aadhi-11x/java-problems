import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        sc.close();
        while (num > 0) {
            int digit = num % 10;
            System.out.print(digit);
            num /= 10;
            if (num > 0) {
                System.out.print(" ");  
            }
        }
    }
}
